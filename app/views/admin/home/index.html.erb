<div class="bg-gray-100  text-6xl">
  <section class="mt-16">
    <%= render partial: 'shared/swap_notices' %>
  </section>
  <section class="mt-16">
    <% if @swap %>
      <%= render partial: 'current_swap_activation' %>
      <%= render partial: 'todays_voucher_supply' %>
    <% else %>
      <%= render partial: 'activate_swap' %>
    <% end %>
  </section>
  <section class="mt-16">
    <%= render partial: 'vouchers_issued' %>
    <%= link_to 'download voucher report', admin_swap_report_url(format: :csv) %>
  </section>
</div>
