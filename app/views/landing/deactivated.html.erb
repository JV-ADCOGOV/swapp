
<!DOCTYPE html>
<html>
  <head>
    <title>Swapp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0"/> <!--320-->
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= stylesheet_pack_tag 'application', media: 'all' %>
    <%= javascript_pack_tag 'application' %>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;600;800&display=swap" rel="stylesheet">
  </head>
  <body class="bg-gray-50 overscroll-x-hidden">
    <%= render partial: 'shared/test_environment' %>
    <div class="min-h-screen">
      <div class="mt-32 flex flex-col justify-center items-center">
        <h1>Oops! Your account has been deactivated.</h1>
        <p class="text-xl">Contact Swapp administrators in order to reactivate.</p>
      </div>
    </div>
  </body>
</html>
